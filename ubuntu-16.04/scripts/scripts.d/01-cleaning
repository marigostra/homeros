remove-pkg $(apt-cache search unity | grep ^unity | cut -f1 -d' ')
remove-pkg $(apt-cache search ubiquity | grep ^ubiquity | cut -f1 -d' ')
remove-pkg $(apt-cache search myspell | grep ^myspell | cut -f1 -d' ')
