remove-pkg $(apt-cache search gir1 | grep ^gir1 | cut -f1 -d' ')
remove-pkg $(apt-cache search libgnome | grep ^libgnome | cut -f1 -d' ')
remove-pkg $(apt-cache search printer-driver | grep ^printer-driver | cut -f1 -d' ')
